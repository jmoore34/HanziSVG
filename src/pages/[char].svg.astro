---
export const prerender = false;
const { char } = Astro.params;
import Character from "../components/Character.astro";
if (import.meta.env.PROD) {
	Astro.response.headers.set("Content-Type", "image/svg+xml");
	Astro.response.headers.set("Cache-Control","public, max-age=604800, immutable");
}
---

<Character
	character={char ?? ""}
	color={Astro.url.searchParams.get("color") ?? undefined}
	outlineColor={Astro.url.searchParams.get("outlineColor") ?? undefined}
/>
